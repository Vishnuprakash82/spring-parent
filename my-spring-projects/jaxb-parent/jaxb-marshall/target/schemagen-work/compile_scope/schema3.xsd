<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="dateAdapter">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="CreateProductRequest">
    <xs:complexContent>
      <xs:extension base="Product">
        <xs:sequence/>
        <xs:attribute name="apikey" type="xs:string"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="Product">
    <xs:sequence>
      <xs:element name="productId" type="xs:integer"/>
      <xs:element name="productDescription" type="xs:string"/>
      <xs:element name="productPrice" type="xs:decimal"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

